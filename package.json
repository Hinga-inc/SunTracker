{
    "name": "suntracker",
    "version": "1.0.0",
    "description": "Track the sun's influence on your local weather",
    "main": "server.js",
    "scripts": {
      "start": "node server.js",
      "dev": "nodemon server.js",
      "client": "cd client && npm start",
      "build": "cd client && npm run build",
      "test": "jest",
      "lint": "eslint .",
      "format": "prettier --write \"**/*.{js,jsx,json,md}\""
    },
    "keywords": [
      "weather",
      "sun",
      "tracking",
      "uv",
      "forecast"
    ],
    "author": "SunTracker Team",
    "license": "MIT",
    "dependencies": {
      "axios": "^0.21.1",
      "dotenv": "^10.0.0",
      "express": "^4.17.1"
    },
    "devDependencies": {
      "eslint": "^7.32.0",
      "jest": "^27.0.6",
      "nodemon": "^2.0.12",
      "prettier": "^2.3.2"
    }
  }