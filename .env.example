# Rename this file to .env and fill in your actual keys

# OpenWeatherMap API for sun & weather data
OPENWEATHERMAP_API_KEY=your_key_here

# MapBox for precise geolocation
MAPBOX_ACCESS_TOKEN=your_token_here

# Optional: Astronomy API for more detailed solar data
ASTRONOMY_API_KEY=your_key_here

# Server configuration
PORT=3000
NODE_ENV=development